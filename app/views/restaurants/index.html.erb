<%= link_to "Create new restaurant", new_restaurant_path  %>

  <h1>Restaurants</h1>

  <ul>

    <% @restaurants.each do |restaurant| %>
      <li><%= restaurant.name %>
        <%= restaurant.address %>
        <%= restaurant.category %>
        <%= link_to 'Show', restaurant %>
      </li>
    <% end %>

  </ul>


